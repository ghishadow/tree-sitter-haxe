{
  "name": "tree-sitter-haxe",
  "version": "0.1.0",
  "description": "A tree sitter parser for haxe.",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter generate && tree-sitter test"
  },
  "author": "vantreeseba@gmail.com",
  "license": "ISC",
  "dependencies": {
    "nan": "^2.15.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.6"
  },
  "tree-sitter": [
    {
      "scope": "source.hx",
      "file-types": [
        "hx"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^(hx|haxe)$"
    }
  ]
}
